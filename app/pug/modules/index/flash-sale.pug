section.flashsale_section
    .container
        .product_block
            .product_block__header
                +heading('FLA<i class="icon-flash-sale"></i>H SALE', 'See All', 'countdown')
            .swiper-container(data-swiper='{"type": "tworowproduct"}')
                .swiper-wrapper
                    -for (var i = 0; i < 30; i++)
                        .swiper-slide
                            +product('one-price', 'slideitem')
                .swiper-button-prev
                .swiper-button-next
